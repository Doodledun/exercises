class Solution {
    public int solution(int[] A) {
        int[] B = new int[A.length];
        
        for (int i = 0; i<A.length; i++){
            int temp = A[i];   
            if (temp <= A.length){
                B[temp-1] = 1;   
            }
        }    
        
        for (int j=0; j < B.length;j++){
            if ( B[j] == 0 )  
                return j+1;
        }
        
        return A.length+1;
    }
}
