
class Solution { 
    
    public int solution(int[] A) {
        // write your code in Java SE 8
       int len = A.length + 1;
       int sum = (1+len)*len/2;
       int temp =0;
       for(int i=0; i< A.length; i++){
              temp += A[i];
       }
       
       return (sum - temp);
    }
    
}
